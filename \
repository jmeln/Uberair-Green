using System;

class Plane()
{
    double plane_fuel,plane_subtractFuel;
    int plane_numPass, plane_timeZone,plane_addPass,plane_removePass;
    public Plan(double Fuel, double subtractFuel, int numPass, int timeZone, int addPass, int removePass)
    {
        this.Fuel = plane_fuel;
        this.subtractFuel = plane_subtractFuel;
        this.numPass = plane_numPass;
        this.timeZone = plane_timeZone;
        this.addPass = plane_addPass;
        this.removePass = plane_removePass;
    }

    double fuel()
    {
        return plane_fuel;
    }

    double subtractFuel()
    {
        return plane_subtractFuel;
    }
    int numPass()
    {
        return plane_numPass;
    }
    int timeZone()
    {
        return plane_timeZone;
    }
    int addPass()
    {
        return plane_addPass;
    }
    int removePass()
    {
        return plane_removePass;
    }
}
